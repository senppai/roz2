function trim(s)
{
    var l=0; var r=s.length -1;
    while(l < s.length && s[l] == ' ')
    {     l++; }
    while(r > l && s[r] == ' ')
    {     r-=1;     }
    return s.substring(l, r+1);
} 

function readElements()
{
	var srchStr = ""
	var strIDVType = ""
	var ni = document.getElementById('myDiv');
	var numi = document.getElementById('theValue');

	if( ni.hasChildNodes())
	{
		var addSpace = 0
		//alert(ni.childNodes.length)
		//alert(numi)
		for(x= 0; x< numi.value; x++)
		{   	
			var option = document.getElementById('my' + eval(x) + 'DivOption');
			var text1  = document.getElementById('my' + eval(x) + 'DivText1');
			var text2  = document.getElementById('my' + eval(x) + 'DivText2');
			var text3  = document.getElementById('my' + eval(x) + 'DivText3');
	    	    	var text4  = document.getElementById('my' + eval(x) + 'DivSPEOption');	    	    	
			var temp   = document.getElementById('myDiv')
						
			if(text2 != null )
				var tempText2 = trim(text2.value)
			if(text3 != null )
				var tempText3 = trim(text3.value)
			if(text4 == null )
				text4 = ''
			
			if(option != null)
			{
				if(text1.value != '' || text2.value != '' ||text3.value != '' || text4.value != '' )
				{
					if(isDateColumn(getColumnType(option.value)) == 'Y')
					{
						if(tempText2 != '')
							tempText2 = formatDate(new Date(getDateFromFormat(tempText2 ,'MM/dd/yyyy')),'yyyy/MM/dd')
						if(tempText3 != '')
							tempText3 = formatDate(new Date(getDateFromFormat(tempText3 ,'MM/dd/yyyy')),'yyyy/MM/dd')
					}

					srchStr = srchStr +  ' '
					if( isRangeColumn(getColumnType(option.value)) == 'Y')
					{
						if(tempText2 == '')
							srchStr = srchStr +  option.value + ':(' + tempText2 + ',' + tempText3  +  ']'
						else if(tempText3 == '')
							srchStr = srchStr +  option.value + ':[' + tempText2 + ',' + tempText3  +  ')'
						else
							srchStr = srchStr +  option.value + ':[' + tempText2 + ',' + tempText3  +  ']'
					}
					else if(option.value == 'VERSION')
					{
						srchStr = srchStr +  option.value + ':' + text1.value
					}					
					else if(option.value == 'SPENDING_CATEGORY')
					{
						srchStr = srchStr +  option.value + ':"' + text4.value + '"'
					}
					else
					{
						srchStr = srchStr +  option.value + ':"' + text1.value + '"'
					}				
				}
			}
		}
	}  
return escape(srchStr)
}

function removeElement(divNum) {
  var d = document.getElementById('myDiv');
  var olddiv = document.getElementById(divNum);
  d.removeChild(olddiv);
}

function appendToParam(url, param, value)
{
	start = url.toString().indexOf(param + '=');
	end = url.toString().indexOf('&', start);
	if (start == -1){
		return url;
	}
	
	if (end == -1){
		end = url.length;
	}
	
	return url.substring(0, end) + ' ' + value +  url.substring(end, url.length)
	
}

function replaceParam(url, param, value)
{
	start = url.toString().indexOf(param);
	end = url.toString().indexOf('&', start);
	if (start == -1){
		return url;
	}
	
	if (end == -1){
		end = url.length;
	}
	
	return url.substring(0, start) + param + '=' +  value +  url.substring(end, url.length)
	
}

function sendtopage(s)
{

var url = this.location.href
if(myform.seacrhType[0].checked){

  	
	url = appendToParam(url, 'q', s)
}

if(myform.seacrhType[1].checked)    
{
	url = replaceParam(url, 'q', s)
     
}
url = url.replace("search.do","fpdsportal")
this.location.replace(url)
}

