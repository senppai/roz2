function hidedefault(oid)
{
	hidelayer( oid + "SPE" )
}

function changerange(oid,num)
{
	var optionb =document.getElementById(oid + "Option");	
	var sel = getColumnType(optionb.options[optionb.selectedIndex].value)
	
        hidedefault(oid)
            
	if(isRangeColumn(sel) == 'Y')
	{
		showlayer( oid + "Box2" )
		hidelayer( oid + "Box1" )
		
	}
	else
	{
		showlayer( oid + "Box1" )
		hidelayer( oid + "Box2" )
		
	}
	if(isDateColumn(sel) == 'Y')
	{
		showlayer( oid + "Date1" )
		showlayer( oid + "Date2" )
	}
	else
	{
		hidelayer( oid + "Date1" )
		hidelayer( oid + "Date2" )
	}
	if(isListColumn(optionb.options[optionb.selectedIndex].value) == 'Y')
	{
	   hidelayer( oid + "Box2" )
	   hidelayer( oid + "Box1" )
	   hidelayer( oid + "Date1" )
	   hidelayer( oid + "Date2" )
	   showlayer( oid + (optionb.options[optionb.selectedIndex].value).substring(0,3),oid,optionb.options[optionb.selectedIndex].value)	  
	}
}



function isRangeColumn(name)
{	
	if(name.indexOf("Date") != -1)
		return 'Y'
	else if(name.indexOf("Number") != -1)
		return 'Y'
	else
		return 'N'
}

function isDateColumn(name)
{	
	if(name.indexOf("Date") != -1)
		return 'Y'
	else
		return 'N'
}

function isListColumn(name)
{
	for(var i=0;i<divElementName.length; i++)
	{
		if(name == divElementName[i])
			return 'Y'	
	}
	return 'N'
}

