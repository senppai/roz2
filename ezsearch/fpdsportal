<script type="text/javascript" language="JavaScript">
function getParent(d) 
{
       return parent.location.protocol+'//'+parent.location.host+d;
}

function pingFPDSInstance()
{
	var source = "";
        if(source=="FPDS" || source=="FPDS.GOV" )
        {
	   var pingImg=document.getElementById('pingImage');
	   var pingSrc=getParent('/common/jsp/keepAlive.jsp');
	   pingImg.src=pingSrc;
        }
}
</script></body></html>