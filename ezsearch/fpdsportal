  <script type="text/javascript" language="JavaScript">
function getHelp(d)
{
	var browser=navigator.appName;
	if (browser == 'Microsoft Internet Explorer')
	{
	     parent.location.reload(parent.location.protocol+'//'+parent.location.host+d);
	}
	else
	{
	    parent.location.replace(parent.location.protocol+'//'+parent.location.host+d);
	}
}
</script>

