<script type="text/javascript" language="JavaScript">
function getParentURL(d)
{
parent.location.replace(parent.location.protocol+'//'+parent.location.host+d);
}

function sendtoindex(s)
{
var url = parent.location.protocol+'//'+parent.location.host
url =  url + '/ezsearch/' + s;
this.location.replace(url);
}

function constructURL(d, applicationSource)
{
        if(applicationSource == 'FFATA')
	d = '/ffata' + d;
        else if(applicationSource == 'ICD')
	d = '/contractdirectory' + d;
        else
	d = '/common' + d;
        parent.location.replace(parent.location.protocol+'//'+parent.location.host+d);
}
function csvTimeout()
{
    var self = this
    if (this.clicked) return false;
    
    greyOutCSV();
    this.clicked = true;
    setTimeout(function() {self.clicked = false; colorizeCSV();}, 30000);

    return true;

	function greyOutCSV()
	{
		document.getElementById("csvImg").setAttribute("style", "display:none");
		document.getElementById("greyCSV").setAttribute("style", "");
	}
	
	function colorizeCSV()
	{
		document.getElementById("greyCSV").setAttribute("style", "display:none");
		document.getElementById("csvImg").setAttribute("style", "");
	}
}
function pdfTimeout()
{
    var self = this
    if (this.clicked) return false;
    
    greyOutPDF();
    this.clicked = true;
    setTimeout(function() {self.clicked = false; colorizePDF();}, 30000);

    return true;

	function greyOutPDF()
	{
		document.getElementById("pdfImg").setAttribute("style", "display:none");
		document.getElementById("greyPDF").setAttribute("style", "");
	}
	
	function colorizePDF()
	{
		document.getElementById("greyPDF").setAttribute("style", "display:none");
		document.getElementById("pdfImg").setAttribute("style", "");
	}
}
